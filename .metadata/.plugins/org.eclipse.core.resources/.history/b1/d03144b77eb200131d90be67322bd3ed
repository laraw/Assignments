package fleet.model.facade;
import fleet.model.*;


public interface FleetSystemModel {

	
	public void addVehicle(String regno, String makeyear, String model, int odo, int serviceinterval, int km);
	public void displayVehicleInfo(Vehicle vcl);
	public void displayAllVehicles();
	public void scheduleJob(Vehicle vcl);
	public void serviceVehicle(Vehicle vcl);
}
